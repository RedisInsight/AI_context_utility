{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      "src/*": [
        "./RedisInsight/redisinsight/api/src/*"
      ],
    }
  },
  "include": [
    "./src/*.ts"
  ]
}
